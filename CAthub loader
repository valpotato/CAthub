local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "CATHUB V1.0", HidePremium = true, SaveConfig = true, ConfigFolder = "CATHUB.config"})

--value
_G.autoclicker = true
_G.autorebirth = true
_G.autohatch = true
_G.selectegg = "ForestEgg"

--functions
function autoclicker()
    while _G.autoclicker == true do
        game:GetService("ReplicatedStorage").Knit.Services.ClickService.RE.ClickRemote:FireServer("Click")
        wait(0.1)
     end
    end

function autohatch()
    while _G.autohatch == true do
        game:GetService("ReplicatedStorage").Knit.Services.EggService.RF.OpenEgg:InvokeServer({["Auto"] = false,["EggType"] = (_G.selectegg),["UpdateType"] = "Open",["Amount"] = 1})
        wait(.01)
    end
end

function autorebirth()
    while _G.autorebirth == true do
        game:GetService("ReplicatedStorage").Knit.Services.RebirthService.RF.RequestRebirth:InvokeServer()
        wait(.01)
     end
    
end

--tab
local FarmTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local EGGTab = Window:MakeTab({
	Name = "EGG",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


--toggle
FarmTab:AddToggle({
	Name = "üñ±Ô∏èAutoclicker",
	Default = false,
	Callback = function(Value)
		_G.autoclicker = Value
        autoclicker()
	end    
})

FarmTab:AddToggle({
	Name = "Autorebirth",
	Default = false,
	Callback = function(Value)
		_G.autorebirth = Value
        autorebirth()
	end    
})

EGGTab:AddToggle({
	Name = "Autohatch",
	Default = false,
	Callback = function(Value)
		_G.autohatch = Value
        autohatch()
	end    
})


--dropdown
EGGTab:AddDropdown({
	Name = "Selectegg",
	Default = "ForestEgg",
	Options = {"ForestEgg", "Egg2M"},
	Callback = function(Value)
		_G.selectegg = Value
        print(_G.selectegg)
	end    
})

--slider
MiscTab:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "walkspeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
	end    
})

--button
MiscTab:AddButton({
	Name = "Cliam Quest",
	Callback = function()
        game:GetService("ReplicatedStorage").Knit.Services.QuestService.RE.Quest:FireServer("ClicksQuest")
  	end    
})

OrionLib:Init()
